- path: ''
  name: kafka_ui_local_example_com
  hostname: kafka-ui.local.example.com
  proxies:
  - servers:
    - backup: false
      port: 8080
      hostname: env_kafka_ui
      check: true
      optional: false
    path: /
  port: 80
- path: ''
  name: pgadmin_local_example_com
  hostname: pgadmin.local.example.com
  proxies:
  - servers:
    - backup: false
      port: 80
      hostname: env_pgadmin
      check: true
      optional: false
    path: /
  port: 80
- path: ''
  name: swagger_ui_local_example_com
  hostname: swagger-ui.local.example.com
  proxies:
  - servers:
    - backup: false
      port: 8080
      hostname: env_swagger_ui
      check: true
      optional: false
    path: /
  port: 80
- path: ''
  name: auth_local_example_com
  hostname: auth.local.example.com
  proxies:
  - servers:
    - backup: false
      port: 8080
      hostname: env_keycloak
      check: true
      optional: false
    path: /
  redirect:
    type: prefix
    prefix: /auth
    code: 301
  port: 80
- path: ''
  name: admin_ui_local_example_com
  hostname: admin-ui.local.example.com
  proxies:
  - servers:
    - backup: false
      port: 80
      hostname: app_admin_ui
      check: true
      optional: true
    - backup: true
      port: 5173
      hostname: host.docker.internal
      check: true
      optional: true
    path: /
  port: 80
- path: ''
  name: api
  hostname: api.local.example.com
  proxies:
  - servers:
    - backup: false
      port: 8080
      hostname: app_assembly_web
      check: true
      optional: true
    - backup: true
      port: 8080
      hostname: host.docker.internal
      check: true
      optional: true
    path: /api/assembly_web
    cors:
      origin: '*'
  - servers:
    - backup: false
      port: 8080
      hostname: host.docker.internal
      check: true
      optional: true
    path: /api/assembly_web
    cors:
      origin: '*'
  - servers:
    - backup: false
      port: 8081
      hostname: host.docker.internal
      check: true
      optional: true
    path: /api/service_basket
    cors:
      origin: '*'
  - servers:
    - backup: false
      port: 8082
      hostname: host.docker.internal
      check: true
      optional: true
    path: /api/service_catalog
    cors:
      origin: '*'
  - servers:
    - backup: false
      port: 8083
      hostname: host.docker.internal
      check: true
      optional: true
    path: /api/service_customers
    cors:
      origin: '*'
  - servers:
    - backup: false
      port: 8084
      hostname: host.docker.internal
      check: true
      optional: true
    path: /api/service_events
    cors:
      origin: '*'
  - servers:
    - backup: false
      port: 8085
      hostname: host.docker.internal
      check: true
      optional: true
    path: /api/service_payments
    cors:
      origin: '*'
  - servers:
    - backup: false
      port: 8086
      hostname: host.docker.internal
      check: true
      optional: true
    path: /api/service_security
    cors:
      origin: '*'
  port: 80

